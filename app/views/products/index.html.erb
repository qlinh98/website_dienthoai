<p id="notice"><%= notice %></p>

<h1>Products</h1>

<table>
  <thead>
    <tr>
      <th>Pro name</th>
      <th>Quantity</th>
      <th>Price input</th>
      <th>Price output</th>
      <th>Img 1</th>
      <th>Img 2</th>
      <th>Img 3</th>
      <th>Category pro</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.pro_name %></td>
        <td><%= product.quantity %></td>
        <td><%= product.price_input %></td>
        <td><%= product.price_output %></td>
        <td><%= product.img_1 %></td>
        <td><%= product.img_2 %></td>
        <td><%= product.img_3 %></td>
        <td><%= product.category_pro_id %></td>
        <td><%= link_to 'Show', product %></td>
        <td><%= link_to 'Edit', edit_product_path(product) %></td>
        <td><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product', new_product_path %>
