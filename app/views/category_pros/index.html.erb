<p id="notice"><%= notice %></p>

<h1>Category Pros</h1>

<table>
  <thead>
    <tr>
      <th>Category name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @category_pros.each do |category_pro| %>
      <tr>
        <td><%= category_pro.category_name %></td>
        <td><%= link_to 'Show', category_pro %></td>
        <td><%= link_to 'Edit', edit_category_pro_path(category_pro) %></td>
        <td><%= link_to 'Destroy', category_pro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Category Pro', new_category_pro_path %>
